{
  "name": "opentutor-admin-cypress",
  "description": "cypress testing",
  "version": "1.0.0",
  "dependencies": {
    "cypress": "^12.13.0",
    "cypress-fill-command": "^1.0.2",
    "typescript": "^5.1.3"
  },
  "scripts": {
    "cy:open": "npx cypress open --env failOnSnapshotDiff=false",
