{
  "name": "opentutor-admin",
  "description": "web ui for grading opentutor sessions",
  "version": "1.0.0",
  "resolutions": {
    "dot-prop": ">= 5.1.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@reach/router": "^1.3.4",
    "axios": "^0.20.0",
    "gatsby": "^2.24.50",
    "gatsby-plugin-env-variables": "^2.0.0",
    "gatsby-plugin-manifest": "^2.4.24",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-typescript": "^2.4.18",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-toastify": "^6.0.8",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "universal-cookie": "^4.0.3",
    "url-join": "^4.0.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/node": "^14.6.1",
    "@types/react": "^16.9.47",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/url-join": "^4.0.0",
    "@types/uuid": "^8.3.0",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-eslint": "10.x",
    "babel-preset-gatsby": "^0.5.6",
    "core-js": "^3.6.5",
    "cypress": "^5.0.0",
    "cypress-fill-command": "^0.1.0",
    "cypress-image-snapshot": "^3.1.1",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "gatsby-cli": "^2.12.88",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-root-import": "^2.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "license-check-and-add": "^3.0.4",
    "npm-check-updates": "^7.1.1",
    "prettier": "^2.1.0",
    "react-refresh": "^0.8.3",
    "react-test-renderer": "^16.13.1",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npx gatsby build --prefix-paths",
    "dependencies-update": "npx ncu -u",
    "develop": "npx gatsby develop",
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx}'",
    "start": "npm run develop",
    "serve": "gatsby serve --prefix-paths",
    "cy:open": "npx cypress open --env failOnSnapshotDiff=false",
    "cy:open:withSnapshots": "npx cypress open",
    "cy:run": "npx cypress run --env failOnSnapshotDiff=false",
    "cy:run:withSnapshots": "npx cypress run",
    "cy:verify": "npx cypress verify",
    "license:fix": "license-check-and-add remove -f license-config.json && license-check-and-add add -f license-config.json",
    "test:license": "license-check-and-add check -f license-config.json",
    "test": "jest",
    "test:all": "npx run test:format && npx run test:lint && npx run test:audit && npx run test:types && npx run test:cypress && npx test",
    "test:audit": "npm audit --level=moderate",
    "test:cypress": "npx run cy:run --spec 'cypress/**/*.spec.js'",
    "test:format": "prettier --check '**/*.{js,jsx,json,ts,tsx}'",
    "test:lint": "eslint src --ext ts --ext tsx --ext js --ext jsx",
    "test:types": "tsc --noEmit",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ictlearningsciences/opentutor-admin.git"
  },
  "bugs": {
    "url": "https://github.com/ictlearningsciences/opentutor-admin/issues"
  }
}
